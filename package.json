{
  "name": "tsc-babel-esbuild-swc-diff",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.5",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.57",
    "esbuild": "0.17.18",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "typescript": "5.0.4"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "npm-run-all --parallel build:*",
    "build:tsc": "tsc && mv ./dist/input.js ./dist/tsc.js",
    "build:babel": "babel input.tsx --out-file dist/babel.js",
    "build:esbuild": "esbuild input.tsx --outfile=dist/esbuild.js --target=es2015 --log-level=silent",
    "build:swc": "swc input.tsx -o dist/swc.js",
    "postbuild": "prettier --write dist"
  }
}
