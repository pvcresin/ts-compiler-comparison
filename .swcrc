{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "target": "es2015",
    "keepClassNames": true,
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "useDefineForClassFields": false
    }
  }
}
